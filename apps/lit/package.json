{
  "name": "lit-tsparticles-demo",
  "version": "2.9.3",
  "private": true,
  "description": "A simple web component",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf my-element.{d.ts,d.ts.map,js,js.map} test/my-element.{d.ts,d.ts.map,js,js.map} test/my-element_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "lit-analyzer && eslint 'src/**/*.ts'",
    "format": "prettier src/* --write",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "es-dev-server --app-index dev/index.html --node-resolve --watch --open",
    "checksize": "rollup -c ; cat my-element.bundled.js | gzip -9 | wc -c ; rm my-element.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "The Polymer Authors",
  "license": "BSD-3-Clause",
  "dependencies": {
    "lit-element": "^3.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@open-wc/testing": "^3.0.0",
    "@open-wc/testing-karma": "^4.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^2.0.0",
    "eslint": "^8.0.0",
    "karma": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^2.0.0",
    "lit-analyzer": "^1.1.9",
    "lit-tsparticles": "^2.9.3",
    "mocha": "^10.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^4.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "sinon": "^15.0.1",
    "tsparticles": "^2.8.0",
    "typescript": "^4.9.4",
    "web-component-analyzer": "^1.0.0"
  }
}
